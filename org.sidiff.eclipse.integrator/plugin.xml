<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="org.sidiff.eclipse.integrator.SidiffShotgun">
      </startup>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.sidiff.eclipse.integrator.preferences.SiDiffPreferences"
            id="org.sidiff.eclipse.integrator.preferences.SiDiffPreferences"
            name="SiDiff">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.sidiff.eclipse.integrator.preferences.PreferenceInitializer">
      </initializer>
   </extension>

</plugin>
